<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">


    <changeSet id="add_column_map_link" author="M.Gogolin">
        <preConditions>
            <not>
                <columnExists tableName="nursery_table" columnName="map_link"/>
            </not>
        </preConditions>
        <addColumn tableName="nursery_table">
            <column name="map_link" type="TEXT"/>
        </addColumn>
    </changeSet>


</databaseChangeLog>